        <script><![CDATA[(function executeRule(current, previous /*null when async*/) {
	var updval = "";
	var gr = new GlideRecord('x_cria_livestock_t_vaccinations_cvi');
	gr.addQuery('sys_id', current.parent);
	gr.query();
	while(gr.next()) {
		var idstring = gr.official_ids;
		var array = idstring.split(",");
		for(var i=0; i<array.length;i++) {
			if(array[i] == previous.official_identification) {
				array[i] = current.official_identification;
			}
		}
		for(var j=0; j<array.length;j++){
			if(updval == ""){
				updval = array[j];
			}
			else{
				updval = (updval + "," + array[j]);
			}
		}
		gr.official_ids = updval;
		gr.update();
	}
})(current, previous);]]></script>
